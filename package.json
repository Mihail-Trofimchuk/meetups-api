    "geolib": "^3.3.4",
    "mustache": "^4.2.0",
    "mustache-express": "^1.3.2",
    "nestjs-rmq": "^2.11.0",
    "nodemailer": "^6.9.6",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.13.0",
    "pdfkit-table": "^0.1.99",
    "puppeteer": "^21.3.8",
