    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.20",
    "@types/express-session": "^1.17.8",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.200",
    "@types/multer": "^1.4.8",
    "@types/mustache-express": "^1.2.3",
    "@types/node": "^20.3.1",
    "@types/passport-google-oauth20": "^2.0.12",
    "@types/passport-jwt": "^3.0.10",
    "@types/pdfkit": "^0.13.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/interfaces(|/.*)$": "<rootDir>/libs/interfaces/src/$1",
      "^@app/db(|/.*)$": "<rootDir>/libs/db/src/$1",
      "^@app/contracts(|/.*)$": "<rootDir>/libs/contracts/src/$1",
      "^@app/types(|/.*)$": "<rootDir>/libs/types/src/$1"
    }
  }
}
